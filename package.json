{
  "name": "earnfrom-backend",
  "version": "1.0.0",
  "description": "TypeScript Express server for EarnFrom application",
  "scripts": {
    "build": "tsc && tsc-alias",
    "build:watch": "tsc --watch",
    "start": "node -r tsconfig-paths/register dist/server.js",
    "dev": "ts-node-dev --respawn --transpile-only -r tsconfig-paths/register --watch src src/server.ts",
    "clean": "rimraf dist",
    "prebuild": "npm run clean"
  },
  "keywords": [
    "typescript",
    "express",
    "nodejs",
    "api"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@types/mongoose": "^5.11.97",
    "auth-fingerprint": "^1.0.20",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "helmet": "^7.0.0",
    "module-alias": "^2.2.3",
    "mongoose": "^8.18.1",
    "morgan": "^1.10.0",
    "node-telegram-bot-api": "^0.66.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/morgan": "^1.9.4",
    "@types/node": "^20.4.5",
    "@types/node-telegram-bot-api": "^0.64.11",
    "nodemon": "^3.1.10",
    "rimraf": "^5.0.1",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsc-alias": "^1.8.16",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.6"
  }
}
